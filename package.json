{
  "name": "mood-lifter",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "13.7.x"
  },
  "description": "MoodLifter is an app that helps uplift a persons mood through music",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix react-client",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix react-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd react-client && npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boocheet/MoodLifter.git"
  },
  "author": "Tahshara Boochee",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.2",
    "pg": "^8.0.2",
    "standard": "^14.3.3",
    "uuid": "^7.0.3"
  }
}
